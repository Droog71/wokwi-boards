{
  /* The name of the board */
  "name": "Franzininho WiFi",

  /* Board version. Increment it whenever you make changes. */
  "version": 1,

  /* One-liner description of the board, it's capabilities, etc. */
  "description": "Development board to evaluate ESP32-S2 Modules",

  /* The name of the person who created this file */
  "author": "Ariella Eliassaf",

  /* Microcontroller name. Valid values: atmega328p, atmega2560, attiny85, rp2040, esp32 */
  "mcu": "esp32-s2",

  /* Fully Qualified Board Name (FQBN) for the Arduino CLI */
  "fqbn": "esp32:esp32:franzininho_wifi_esp32s2",

  /* Width of the board graphics, in mm. Must match the width defined in board.svg */
  "width": 29.88,

  /* Height of the board graphics, in mm. Must match the height defined in board.svg */
  "height": 69.7,

  /* The pins available on the board.
     "x"/"y" positions are in mm, and are relative to the top-left corner of the board.
     "target" is either:
     - an MCU pin name
     - "GND" for ground
     - "power(n)" for power supply pins, where n is the voltage. e.g. "power(3.3)"
  */
  "pins": {
    "0": { "x": 1.27, "y": 8.34, "target": "GPIO0" },
    "1": { "x": 1.27, "y": 10.88, "target": "GPIO1" },
    "2": { "x": 1.27, "y": 13.42, "target": "GPIO2" },
    "3": { "x": 1.27, "y": 15.96, "target": "GPIO3" },
    "4": { "x": 1.27, "y": 18.5, "target": "GPIO4" },
    "5": { "x": 1.27, "y": 21.04, "target": "GPIO5" },
    "6": { "x": 1.27, "y": 23.58, "target": "GPIO6" },
    "7": { "x": 1.27, "y": 26.12, "target": "GPIO7" },
    "8": { "x": 1.27, "y": 28.66, "target": "GPIO8" },
    "9": { "x": 1.27, "y": 31.2, "target": "GPIO9" },
    "10": { "x": 1.27, "y": 33.74, "target": "GPIO10" },
    "11": { "x": 1.27, "y": 36.28, "target": "GPIO11" },
    "12": { "x": 1.27, "y": 38.82, "target": "GPIO12" },
    "13": { "x": 1.27, "y": 41.36, "target": "GPIO13" },
    "14": { "x": 1.27, "y": 43.9, "target": "GPIO14" },
    "15": { "x": 1.27, "y": 46.44, "target": "GPIO15" },
    "16": { "x": 1.27, "y": 48.98, "target": "GPIO16" },
    "17": { "x": 1.27, "y": 51.52, "target": "GPIO17" },
    "GND.1": { "x": 1.27, "y": 54.06, "target": "GND" },
    "3V3": { "x": 1.27, "y": 56.3, "target": "power(3.3)" },

    "46": { "x": 29, "y": 8.34, "target": "GPIO46" },
    "45": { "x": 29, "y": 10.88, "target": "GPIO45" },
    "RX": { "x": 29, "y": 13.42, "target": "GPIO44" },
    "TX": { "x": 29, "y": 15.96, "target": "GPIO43" },
    "42": { "x": 29, "y": 18.5, "target": "GPIO42" },
