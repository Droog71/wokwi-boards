  /* On-board LED definitions. These only draw the light of the LED when it's on. 
     You should draw the body of the LED in your .svg file. */
  "leds": [
    {
      "id": "power",
      "x": 13.9,
      "y": 38,
      "type": "0603",
      "color": "red",
      "pins": {
        "A": "5V", // This is a power LED, so it's always on
        "C": "GND.1"
      }
    },
    {
      "id": "rgb",
      "x": 20.8,
      "y": 36,
      "type": "ws2812",
      "pins": {
        "DI": "GPIO18"
      }
    }
  ]
